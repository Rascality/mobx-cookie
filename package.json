{
  "name": "mobx-cookie",
  "version": "3.0.0",
  "description": "Syncs a cookie's value with a MobX observable, allowing observers to react to its changes.",
  "repository": "https://github.com/will-stone/mobx-cookie.git",
  "license": "MIT",
  "author": "Will Stone <contact@wstone.io>",
  "main": "lib/mobx-cookie.js",
  "scripts": {
    "build": "tsc",
    "lint": "ws-scripts lint",
    "start": "parcel demo/index.html",
    "test": "jest",
    "test-watch": "jest --watchAll",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "watch": "tsc -w"
  },
  "husky": {
    "hooks": {
      "pre-commit": "ws-scripts pre-commit"
    }
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/ws-scripts/dist/config/eslintrc"
    ]
  },
  "dependencies": {
    "js-cookie": "^2.2.1"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/js-cookie": "^2.2.5",
    "@types/mockdate": "^2.0.0",
    "@types/node": "^13.9.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "jest": "^25.1.0",
    "mobx": "^5.15.4",
    "mobx-react": "^6.1.8",
    "mockdate": "^2.0.5",
    "parcel-bundler": "^1.12.4",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3",
    "ws-scripts": "^1.12.0"
  },
  "peerDependencies": {
    "mobx": "^5.0.0"
  }
}
